{
  "name": "sf",
  "type": "module",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@wdio/allure-reporter": "^9.19.1",
    "@wdio/cli": "^9.19.1",
    "@wdio/local-runner": "^9.19.1",
    "@wdio/mocha-framework": "^9.19.1",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.3.3",
    "wdio-wait-for": "^3.1.0"
  },
  "scripts": {
    "wdio": "wdio run ./wdio.conf.ts",
    "wdio:chrome": "BROWSER=chrome wdio run ./wdio.conf.ts",
    "wdio:firefox": "BROWSER=firefox wdio run ./wdio.conf.ts",
    "wdio:safari": "BROWSER=safari wdio run ./wdio.conf.ts",
    "wdio:edge": "BROWSER=MicrosoftEdge wdio run ./wdio.conf.ts",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure": "npm run wdio && npm run allure:report && npm run allure:open",
    "playwright:test": "npx playwright test && npm run allure:report && npm run allure:open",
    "playwright:test:devnet": "npx playwright test test/api/devnetTokenValidation.spec.ts",
    "playwright:test:sol": "npx playwright test test/api/solTokenValidation.spec.ts",
    "playwright:test:switch": "npx playwright test test/api/switchingNetwork.spec.ts",
    "playwright:test:break": "npx playwright test test/api/breakApi.spec.ts"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}